    "electron": "^2.0.18",
    "electron-builder": "^21.2.0",
    "electron-packager": "^14.1.1",
    "electron-prebuilt": "^1.4.13",
    "electron-rebuild": "^1.8.8",
    "node-gyp": "^6.0.1"
