{
  "name": "dodoc",
  "productName": "do•doc",
  "version": "9.3.2-dev",
  "description": "do•doc is a documentation tool for creativity.",
  "author": "L’Atelier des Chercheurs <https://latelier-des-chercheurs.fr/>",
  "homepage": "https://github.com/l-atelier-des-chercheurs/dodoc#readme",
  "license": "AGPL-3.0-only",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/l-atelier-des-chercheurs/dodoc.git"
  },
  "bugs": {
    "url": "https://github.com/l-atelier-des-chercheurs/dodoc/issues"
  },
  "scripts": {
    "start": "electron .",
    "debug": "electron --inspect=5858 . -- --debug --verbose",
    "node-start": "node .",
    "node-debug": "node . --inspect=5858 -- --debug --verbose",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "engines": {
    "node": "16.14.2",
    "npm": "*"
  },
  "build": {
    "electronVersion": "19.0.4",
    "appId": "dodoc",
    "productName": "do•doc",
    "mac": {
      "category": "public.app-category.education"
    },
    "linux": {
      "target": "AppImage"
    },
    "appImage": {
      "synopsis": "A documentation tool for creativity.",
      "category": "Game",
      "desktop": "dodoc-linux-desktop-entry.desktop"
    }
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "archiver": "^5.3.1",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "cheerio": "^0.22.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "electron-pdf-window": "^1.0.12",
    "electron-window-state": "^5.0.3",
    "eventemitter2": "^6.4.5",
    "exif-reader": "^1.0.3",
    "express": "^4.18.1",
    "ffmpeg-static": "^5.0.0",
    "ffprobe-static": "github:JingeXC/ffprobe-static",
    "fluent-ffmpeg": "^2.1.2",
    "formidable": "^2.0.1",
    "fs-extra": "^10.1.0",
    "ip-anonymize": "^0.1.0",
    "lru-cache": "^7.10.1",
    "node-fetch": "^2.6.7",
    "pad-left": "^2.1.0",
    "path": "^0.12.7",
    "pdf-extractor": "^2.1.2",
    "platform-folders": "^0.6.0",
    "portscanner": "^2.2.0",
    "pug": "^3.0.2",
    "rich-text": "^4.1.0",
    "sharedb": "^1.5.2",
    "sharedb-logger": "^0.1.4",
    "sharp": "^0.30.6",
    "slugg": "^1.2.1",
    "socket.io": "^4.5.1",
    "stl-thumbnailer-node": "^1.2.1",
    "uuid": "^8.3.2",
    "validator": "^13.7.0",
    "websocket-json-stream": "0.0.3",
    "write-file-atomic": "^4.0.1",
    "ws": "^8.8.0"
  },
  "devDependencies": {
    "electron": "19.0.4",
    "electron-builder": "^23.0.3",
    "electron-rebuild": "^3.2.7",
    "node-gyp": "^9.0.0"
  }
}
