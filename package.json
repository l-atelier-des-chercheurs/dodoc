{
  "name": "dodoc",
  "productName": "do•doc",
  "version": "7.7.0-dev",
  "description": "do•doc is a documentation tool for creativity.",
  "author": "L’Atelier des Chercheurs <https://latelier-des-chercheurs.fr/>",
  "homepage": "https://github.com/l-atelier-des-chercheurs/dodoc#readme",
  "license": "CC-BY-NC-SA-4.0",
  "main": "main.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/l-atelier-des-chercheurs/dodoc.git"
  },
  "bugs": {
    "url": "https://github.com/l-atelier-des-chercheurs/dodoc/issues"
  },
  "scripts": {
    "start": "electron .",
    "debug": "electron --inspect=5858 . --debug --verbose",
    "debug-brk": "electron --inspect=5858 . --debug-brk --verbose",
    "node-start": "node .",
    "node-debug": "node . --inspect=5858 --debug --verbose",
    "pack": "build --dir",
    "dist": "build",
    "postinstall": "electron-builder install-app-deps"
  },
  "engines": {
    "node": "8.2.1",
    "npm": "*"
  },
  "build": {
    "electronVersion": "2.0.5",
    "appId": "dodoc2",
    "productName": "do•doc",
    "asar": false,
    "mac": {
      "category": "public.app-category.education"
    }
  },
  "dependencies": {
    "archiver": "^3.0.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "dodoc-parsedown": "git://github.com/louis-ev/dodoc-parsedown.git#master",
    "electron-context-menu": "^0.10.1",
    "electron-devtools-installer": "^2.2.3",
    "electron-log": "^2.2.17",
    "electron-pdf-window": "^1.0.12",
    "exif-reader": "^1.0.2",
    "express": "^4.16.4",
    "ffmpeg-static": "^2.2.0",
    "ffprobe-static": "^3.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "formidable": "^1.2.0",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "gulp-util": "^3.0.8",
    "moment": "^2.22.2",
    "node-localstorage": "^1.3.1",
    "pad-left": "^2.1.0",
    "parser-readline": "^1.0.2",
    "path": "^0.12.7",
    "platform-folders": "^0.2.7",
    "portscanner": "^2.2.0",
    "pug": "^2.0.2",
    "sharp": "^0.21.0",
    "slugg": "^1.2.1",
    "socket.io": "^2.1.1",
    "source-map": "^0.7.3",
    "validator": "^10.9.0",
    "vue": "^2.5.17",
    "vue-i18n": "^8.3.1",
    "write-file-atomic": "^2.3.0"
  },
  "devDependencies": {
    "electron": "2.0.5",
    "electron-builder": "^20.35.0",
    "electron-packager": "^12.2.0",
    "electron-prebuilt": "^1.4.13",
    "electron-rebuild": "^1.8.2",
    "node-gyp": "^3.8.0"
  }
