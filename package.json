{
  "name": "dodoc",
  "productName": "do•doc",
  "version": "9.1.2",
  "description": "do•doc is a documentation tool for creativity.",
  "author": "L’Atelier des Chercheurs <https://latelier-des-chercheurs.fr/>",
  "homepage": "https://github.com/l-atelier-des-chercheurs/dodoc#readme",
  "license": "AGPL-3.0-only",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/l-atelier-des-chercheurs/dodoc.git"
  },
  "bugs": {
    "url": "https://github.com/l-atelier-des-chercheurs/dodoc/issues"
  },
  "scripts": {
    "start": "electron .",
    "debug": "electron --inspect=5858 . --debug --verbose",
    "debug-brk": "electron --inspect=5858 . --debug-brk --verbose",
    "node-start": "node .",
    "node-debug": "node . --inspect=5858 --debug --verbose",
    "node-debug-brk": "node . --inspect=5858 --debug-brk --verbose",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "engines": {
    "node": "12.18.3",
    "npm": "*"
  },
  "build": {
    "electronVersion": "11.0.3",
    "appId": "dodoc2",
    "productName": "do•doc",
    "asar": false,
    "mac": {
      "category": "public.app-category.education"
    },
    "linux": {
      "target": "AppImage"
    },
    "appImage": {
      "synopsis": "A documentation tool for creativity.",
      "category": "Game",
      "desktop": "dodoc-linux-desktop-entry.desktop"
    }
  },
  "dependencies": {
    "archiver": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cheerio": "^0.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^2.3.4",
    "csv-writer": "^1.6.0",
    "dodoc-parsedown": "github:louis-ev/dodoc-parsedown#master",
    "electron-pdf-window": "^1.0.12",
    "electron-window-state": "^5.0.3",
    "exif-reader": "^1.0.3",
    "express": "^4.17.1",
    "ffmpeg-ffprobe-static": "*",
    "fluent-ffmpeg": "^2.1.2",
    "formidable": "^1.2.2",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "gulp-util": "^3.0.8",
    "ip-anonymize": "^0.1.0",
    "memory-cache": "^0.2.0",
    "moment": "^2.29.4",
    "node-abi": "^2.19.3",
    "node-localstorage": "^2.1.6",
    "pad-left": "^2.1.0",
    "path": "^0.12.7",
    "platform-folders": "^0.5.2",
    "portscanner": "^2.2.0",
    "pug": "^3.0.0",
    "quill-render": "^1.0.5",
    "rich-text": "^4.1.0",
    "sharedb": "^1.5.2",
    "sharedb-logger": "^0.1.4",
    "sharp": "^0.26.3",
    "slugg": "^1.2.1",
    "socket.io": "^2.3.0",
    "source-map": "^0.7.3",
    "spark-md5": "^3.0.1",
    "uuid": "^8.3.1",
    "validator": "^13.5.1",
    "websocket-json-stream": "0.0.3",
    "write-file-atomic": "^3.0.3",
    "ws": "^7.4.0"
  },
  "devDependencies": {
    "electron": "11.0.3",
    "electron-builder": "^22.9.1",
    "electron-packager": "^15.1.0",
    "electron-rebuild": "^2.3.4",
    "node-gyp": "^7.1.2"
  }
}
