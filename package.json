    "portscanner": "^2.2.0",
    "pug": "^2.0.4",
    "quill-render": "^1.0.5",
    "rich-text": "^4.0.0",
    "sharedb": "^1.0.0",
    "sharedb-logger": "^0.1.4",
    "sharp": "^0.23.4",
