    "instascan": "^1.0.0",
    "jquery": "^3.5.1",
    "katex": "^0.12.0",
    "linkifyjs": "^2.1.9",
    "moment": "^2.29.1",
    "node-lessify": "^0.1.5",
    "node-sass": "^4.14.1",
    "portal-vue": "^1.5.1",
    "prismjs": "^1.24.0",
    "quill": "^1.3.7",
